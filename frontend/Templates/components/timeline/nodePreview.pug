//- Development Preview Timeline (for design testing)
.timeline-preview.timeline-preview--dev(data-variant="dev-preview")
  .timeline-visual.timeline-visual--dev
    .timeline-track
    .timeline-connector
    .timeline-nodes-container
      .timeline-node.timeline-node--status-1.timeline-node--executed(data-status-code="1", data-step="1")
        .node-date(data-month="Nov", data-day="10")
          span.month Nov
          span.day 10
        .node-icon
          .node-circle
        .node-info
          .node-status Executed
          p.node-time 09:00
      .timeline-node.timeline-node--status-2.timeline-node--processing(data-status-code="2", data-step="2")
        .node-date(data-month="TBD", data-day="")
          span.month TBD
          span.day
        .node-icon
          .node-circle
        .node-info
          .node-status Processing
          p.node-time --:--
      .timeline-node.timeline-node--status-3.timeline-node--international-transit(data-status-code="3", data-step="3")
        .node-date(data-month="TBD", data-day="")
          span.month TBD
          span.day 
        .node-icon
          .node-circle
        .node-info
          .node-status Intl In Transit
          p.node-time --:--
      .timeline-node.timeline-node--status-4.timeline-node--scheduled(data-status-code="4", data-step="4")
        .node-date(data-month="TBD", data-day="")
          span.month TBD
          span.day
        .node-icon
          .node-circle
        .node-info
          .node-status Scheduled
          p.node-time --:--
      .timeline-node.timeline-node--status-5.timeline-node--order-completed(data-status-code="5", data-step="5")
        .node-date(data-month="Nov", data-day="12")
          span.month Nov
          span.day 12
        .node-icon
          .node-circle
        .node-info
          .node-status Order Completed
          p.node-time 17:45
      .timeline-node.timeline-node--status-6.timeline-node--order-final(data-status-code="6", data-step="6")
        .node-date(data-month="Nov", data-day="13")
          span.month Nov
          span.day 13
        .node-icon
          .node-circle
        .node-info
          .node-status Final Delivered
          p.node-time 09:10
